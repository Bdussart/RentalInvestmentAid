{
  "Serilog": {
    "Using": [ "Serilog.Sinks.Console", "Serilog.Sinks.File" ],
    "MinimumLevel": {
      "Default": "Information",
      "Override": {
        "Microsoft": "Warning",
        "System": "Warning"
      }
    },
    "WriteTo": [
      {
        "Name": "Console"
      },
      {
        "Name": "File",
        "Args": {
          "path": "C:/Logs/Rental/ConsoleApp/log-development-.txt",
          "rollingInterval": "Day",
          "rollOnFileSizeLimit": true,
          "formatter": "Serilog.Formatting.Compact.CompactJsonFormatter, Serilog.Formatting.Compact"
        }
      }
    ],
    "Enrich": [ "FromLogContext", "WithMachineName", "WithProcessId", "WithThreadId" ],
    "Properties": {
      "Environment": "Development"
    }
  },
  "ConnectionStrings": {
    "RentalInvestmentAidDatabase": "Data Source=127.0.0.1,1433;Initial Catalog=RentalInvestmentAid;Persist Security Info=True;User ID=RentalUser;Password=AbC12345678!;Encrypt=False"
    //"RentalInvestmentAidDatabase": "Data Source=127.0.0.1,1455;Initial Catalog=RentalInvestmentAid;Persist Security Info=True;User ID=RentalUser;Password=AbC12345678!;Encrypt=False"
  },
  "Cache": {
    "RentalCacheKey": "RentalCacheKey",
    "AnnouncementCacheKey": "AnnouncementCacheKey",
    "RateCacheKey": "RateCacheKey",
    "CityCacheKey": "CityCacheKey",
    "LoanCacheKey": "LoanCacheKey",
    "RentCacheKey": "RentCacheKey"
  },
  "Queue": {
    "RentalQueueName": "rental-dev",
    "AnnouncementQueueName": "announcement-dev"
  },
  "exclude": [
    "**/bin",
    "**/bower_components",
    "**/jspm_packages",
    "**/node_modules",
    "**/obj",
    "**/platforms"
  ]
}